public class UserInputProgram{
    public static double secantSqrt(double N, double epsilon) {
        if(N < 0){
            return -1;  
        }
        if(epsilon <= 0){
            return -2; 
        }
        double x0 = N / 3.0;
        double x1 = N / 3.0;
        while(Math.abs(x1 * x1 - N) >= epsilon){
            double f_x0 = x0 * x0 - N;
            double f_x1 = x1 * x1 - N;
            double x2 = x1 - f_x1 * (x1 - x0) / (f_x1 - f_x0);
            x0 = x1;
            x1 = x2;
        }
        return x1; 
    }
    public static void main(String[] args){
        Scanner scanner = new Scanner(System.in);
        double N = scanner.nextDouble();
        double epsilon = scanner.nextDouble();
        double result = secantSqrt(N, epsilon);
        if(result == -1){
            System.out.println("Import illegal");
        }else if(result == -2){
            System.out.println("Import illegal");
        }else{
            System.out.printf("Result: %.6f\n", result);
        }
        scanner.close();
    }
}



public class BisectionSqrt{
    public static double bisectionSqrt(double N, double epsilon) {
        if(N < 0){
            return -1; 
        }else if(epsilon <= 0){
            return -1;  
        }
        double low = 0, high = Math.max(1, N);  
        while(((low + high) / 2)*((low + high) / 2)-N > epsilon){
            double mid = (low + high) / 2;
            if(mid * mid < N){
                low = mid; 
            }else{
                high = mid;
            }
        }
        return (low + high) / 2;  
    }
    public static void main(String[] args){
        Scanner scanner = new Scanner(System.in);
        double N = scanner.nextDouble();
        double epsilon = scanner.nextDouble();
        double result = bisectionSqrt(N, epsilon);
        if(result == -1){
            System.out.println("Invalid input. Negative numbers have no real square roots");
        }else if(result == -2){
            System.out.println("Invalid input");
        }else{
            System.out.println(result);
        }
        scanner.close();
    }
}


